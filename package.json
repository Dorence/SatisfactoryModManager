{
  "name": "satisfactory-mod-manager-gui",
  "version": "2.4.1",
  "author": "mircearoata",
  "description": "Handles Satisfactory mods and mod loader",
  "license": "GPL-3.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "postinstall": "yarn lint && electron-builder install-app-deps",
    "dist:win": "yarn build && electron-builder build -w nsis",
    "dist:linux": "yarn build && electron-builder build -l appImage",
    "dist": "node ./dist/distPlatform.js"
  },
  "build": {
    "productName": "Satisfactory Mod Manager",
    "appId": "com.mircearoata.smmanagergui",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*",
      "icons/*"
    ],
    "protocols": {
      "name": "Satisfactory Mod Manager",
      "schemes": [
        "smmanager"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "icons/icon.icns"
    },
    "win": {
      "icon": "icons/icon.ico"
    },
    "linux": {
      "icon": "icons",
      "category": "Game"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "${productName} Setup.${ext}"
    },
    "portable": {
      "artifactName": "${productName} Portable.${ext}"
    },
    "appImage": {
      "artifactName": "${productName}.${ext}"
    },
    "asarUnpack": [
      "**/node_modules/exiftool-vendored.*/**/*"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "debounce": "^1.2.1",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.3.2",
    "electron-updater": "^4.3.8",
    "filenamify": "^4.1.0",
    "fuse.js": "^6.4.6",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "jszip": "^3.6.0",
    "marked": "^2.0.1",
    "node-stream-zip": "^1.13.2",
    "platform-folders": "^0.5.2",
    "sanitize-html": "^2.3.3",
    "satisfactory-mod-manager-api": "^2.2.1",
    "semver": "^7.3.5",
    "socket.io": "^4.0.0",
    "vue": "^2.5.16",
    "vue-apollo": "^3.0.7",
    "vue-async-computed": "^3.9.0",
    "vue-electron": "^1.0.6",
    "vuetify": "^2.4.8",
    "vuex": "^3.6.2",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-proposal-function-bind": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.8",
    "@babel/runtime-corejs2": "^7.13.10",
    "@mdi/font": "^5.9.55",
    "ajv": "^7.2.3",
    "babel-loader": "^8.2.2",
    "cfonts": "^2.9.1",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "deepmerge": "^4.2.2",
    "del": "^6.0.0",
    "devtron": "^1.4.0",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.0.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.8.0",
    "eslint-webpack-plugin": "^2.5.3",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "listr": "^0.14.3",
    "mini-css-extract-plugin": "^1.3.9",
    "node-loader": "^1.0.2",
    "node-sass": "^5.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^5.7.3"
  }
}
